{
  "name": "Producktivity",
  "description": "Duck makes sure you keep on track.",
  "version": "1.0",
  "icons":{
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  },
  "manifest_version": 3,
  "default_icon": "icon.png",
  "action": {
      "default_icon":{
              "16": "icon.png",
              "48": "icon.png",
              "128": "icon.png"
      },
      "default_popup": "index.html",
      "default_title": "converse with duck"
  },
  "permissions":[
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*/"
  ]

}   